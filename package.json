{
  "name": "terra-ui",
  "version": "0.1.0",
  "description": "terra-ui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cerner/terra-ui.git"
  },
  "keywords": [
    "Cerner",
    "Terra",
    "Terra UI",
    "terra-ui"
  ],
  "author": "Cerner Corporation",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/cerner/terra-ui/issues"
  },
  "jest": {
    "collectCoverageFrom": [
      "packages/**/src/*.js",
      "packages/**/src/*.jsx"
    ],
    "setupFiles": [
      "./jestsetup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "compile": "lerna run compile",
    "lint": "lerna run lint",
    "lint:js": "lerna run lint:js",
    "lint:scss": "lerna run lint:scss",
    "jest": "jest",
    "jest:coverage": "jest --coverage",
    "test": "lerna exec --concurrency 1 npm test"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "babel-cli": "^6.18.0",
    "babel-jest": "^18.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.0",
    "enzyme-to-json": "^1.4.5",
    "eslint": "^3.13.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^18.1.0",
    "lerna": "2.0.0-beta.36",
    "node-sass": "^4.3.0",
    "postcss-loader": "^1.2.2",
    "react-addons-test-utils": "^15.4.2",
    "react-storybook-addon-props-combinations": "^0.3.0",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "stylelint": "^7.8.0",
    "stylelint-config-sass-guidelines": "^2.0.0",
    "terra-application": "^0.2.0",
    "terra-legacy-theme": "^0.1.0",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "terra-mixins": "^1.0.0"
  }
}
