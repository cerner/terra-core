{
  "name": "terra-site",
  "version": "1.8.0",
  "description": "Documentation Site for Functional Verification",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cerner/terra-core.git"
  },
  "author": "Cerner Corporation",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/cerner/terra-core/issues"
  },
  "homepage": "https://github.com/cerner/terra-core#readme",
  "scripts": {
    "clean": "lerna exec --scope terra-site -- rimraf node_modules",
    "start": "webpack-dev-server --progress",
    "compile": "npm run compile:clean && npm run compile:build",
    "compile:clean": "lerna exec --scope terra-site -- rimraf lib",
    "compile:build": "lerna exec --scope terra-site -- babel src --out-dir lib --copy-files",
    "compile:heroku": "webpack --config webpack.prod.config --progress",
    "compile:prod": "webpack --config webpack.prod.config -p",
    "compile:prod-stats": "webpack --config webpack.prod.config -p --json > stats.json",
    "deploy": "npm run compile:prod && gh-pages -d build",
    "lint": "npm run lint:js",
    "lint:js": "lerna exec --scope terra-site -- eslint --ext .js,.jsx . --ignore-path ../../.eslintignore",
    "test": "npm run test:spec",
    "test:spec": "lerna exec --scope terra-site -- jest --config ../../jestconfig.json"
  },
  "dependencies": {
