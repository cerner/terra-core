    "terra-content-container": "^0.x",
    "terra-date-picker": "^0.x",
    "terra-demographics-banner": "^0.4.0",
    "terra-form": "^0.1.0",
    "terra-grid": "^2.4.1",
    "terra-i18n": "^0.x",
    "terra-i18n-plugin": "^0.x",
    "terra-icon": "^0.x",
    "terra-image": "^0.x",
    "terra-legacy-theme": "^0.x",
    "terra-list": "^0.5.0",
    "terra-markdown": "^0.x",
    "terra-mixins": "^1.0.1",
    "terra-modal": "^0.x",
    "terra-progress-bar": "^0.x",
    "terra-props-table": "^0.x",
    "terra-responsive-element": "^0.x",
    "terra-search-field": "^0.1.0",
    "terra-slide-panel": "^0.x",
    "terra-status": "^0.x",
    "terra-table": "^0.6.0",
    "terra-time-input": "^0.1.1"
