  "devDependencies": {
    "terra-toolkit": "^0.3.1"
  },
  "peerDependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-onclickoutside": "^5.11.1",
    "tether": "^1.4.0",
    "terra-base": "^0.4.1",
    "terra-icon": "^0.4.1",
    "terra-mixins": "^1.1.1"
